{
  "name": "gf-payments",
  "version": "0.0.1",
  "description": "Gofundraise Paydock Payment module",
  "author": "Srini Vasudevan <srini@gofundraise.com.au>",
  "main": "dist/gf-payments.min.js",
  "license": "ISC",
  "scripts": {
    "lint": "eslint src/**/*.vue",
    "build": "npm run build:cjs && npm run build:umd",
    "build:cjs": "poi build --format cjs --dist dist",
    "build:umd": "poi build --format umd --dist dist/umd",
    "build:doc": "vue-styleguidist build",
    "serve": "vue-styleguidist server",
    "test": "poi test",
    "test:watch": "poi test --watch",
    "test:cov": "poi test --coverage"
  },
  "keywords": [
    "vue",
    "component"
  ],
  "files": [
    "src/",
    "dist/"
  ],
  "dependencies": {
    "@paydock/client-sdk": "^1.1.16",
    "axios": "^0.18.0",
    "braintree-web": "^3.34.0",
    "vue": "^2.5.3",
    "vue-stripe-elements-plus": "^0.2.8"
  },
  "devDependencies": {
    "babel-plugin-istanbul": "^4.1.1",
    "babel-plugin-transform-runtime": "^6.22.0",
    "babel-preset-env": "^1.3.2",
    "babel-preset-stage-2": "^6.22.0",
    "chai": "^4.1.2",
    "eslint": "^4.11.0",
    "eslint-plugin-vue": "beta",
    "jquery": "^3.3.1",
    "karma-chai": "^0.1.0",
    "karma-chrome-launcher": "^2.2.0",
    "karma-phantomjs-launcher": "^1.0.4",
    "karma-phantomjs-shim": "^1.5.0",
    "node-glob": "^1.2.0",
    "poi": "^9.5.2",
    "poi-preset-karma": "^9.2.1",
    "prettier": "1.12.1",
    "typescript": "^2.6.1",
    "vue": "^2.5.16",
    "vue-styleguidist": "^1.4.4",
    "vue-webpack-loaders": "^1.0.6",
    "webpack": "^3.1.0"
  }
}
